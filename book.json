{
    "plugins": [
      "highlight",
      "katex",
      "-search",                 
      "-lunr",                  
      "-sharing",
      "-search-pro",
      "expandable-chapters-small",
      "-splitter",
      "back-to-top-button",
      "github",
      "code",
      "@honkit/honkit-plugin-theme"
    ],
    "pluginsConfig": {
      "katex": {
        "inlineMath":  [ ["$", "$"], ["\\(", "\\)"] ],
        "displayMath": [ ["$$","$$"], ["\\[","\\]"] ],
        "throwOnError": false,
        "strict": "warn",          
        "macros": { "\\E": "\\mathbb{E}" }
      },
      "highlight": {
        "lang": { "eval_rst": "rst", "toc": "text" }
      },
      "github": { "url": "https://github.com/KittenCN" }
  }
}
  